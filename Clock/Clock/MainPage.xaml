                    <LinearGradientBrush >
                        <GradientStop Color="#FF000000" Offset="0.9"/>
                        <GradientStop  Offset="0.99"/>
                        <GradientStop Color="#FF696969" Offset="1"/>
                    </LinearGradientBrush>
                </Ellipse.Stroke>
